<launch>

	<arg name="map_file" default="$(find lab5_test)/map/empty.yaml"/>
	<node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)">
		<remap from="map" to="map_raw"/>
	</node>	

	<node pkg="lab4" name="map_cal" type="map_cal"/>

	<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
		<param name="port" value="/dev/ttyACM0"/>
	</node>
	
	<node pkg="lab5_test" name="sim_subgoal" type="sim_subgoal"/>

	<node pkg="lab5" name="tf_pos" type="tf_pos"/>

	<node pkg="lab4" name="nav" type="nav"/>

	<node pkg="rviz" name="rviz" type="rviz"/>

</launch>
